{
  "compilerOptions": {
    "jsx": "react",
    "target": "es2016", 
    "module": "commonjs",
    "moduleResolution": "node",    
    "isolatedModules": true,                            
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,            
    "strict": true,  
    "skipLibCheck": true,
    "typeRoots": ["./node_modules/@types", "./src/types"]
  }
}
